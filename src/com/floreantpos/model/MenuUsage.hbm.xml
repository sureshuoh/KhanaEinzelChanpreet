<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
	<class lazy="false" name="com.floreantpos.model.MenuUsage" optimistic-lock="version" table="MENU_USAGE">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		<property column="CATEGORY" name="category" type="java.lang.String" />
		<property column="COUNT" name="count" type="java.lang.Integer" />
		<property column="GROSSSALES" name="grossSales" type="java.lang.Double" />
		<property column="TAX" name="tax" type="java.lang.Integer" />
	</class>
</hibernate-mapping>
