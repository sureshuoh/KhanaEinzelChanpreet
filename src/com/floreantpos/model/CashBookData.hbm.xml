<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
	<class lazy="false" name="com.floreantpos.model.CashBookData" table="CASHBOOKDATA">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		<property name="beschreibung" column="BESCHREIBUNG" length = "200" type="java.lang.String" />
		<property name="einzahlung" column="EINZAHLUNG" length = "200" type="java.lang.Boolean" />
		<property name="auszahlung" column="AUSZAHLUNG" length = "200" type="java.lang.Boolean" />
		<property name="konto" column="KONTO_NR" length = "10" type="java.lang.Integer" />		
   </class>
</hibernate-mapping>
