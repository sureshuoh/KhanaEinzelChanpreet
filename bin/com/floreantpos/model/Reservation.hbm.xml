<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
	<class lazy="false" name="com.floreantpos.model.Reservation" table="RESERVATION">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		<property name="date" column="DATE" length="8" type="java.lang.String" />
		<property name="email" column="EMAIL" type="java.lang.String" />
		<property name="message" column="MESSAGE" type="java.lang.String" />
		<property name="name" column="NAME" type="java.lang.String" />
		<property name="person" column="PERSON" type="java.lang.Integer" />
		<property name="worker" column="WORKER" type="java.lang.String" />
		<property name="telefon" column="TELEFON" type="java.lang.String" />
		<property name="time" column="TIME" type="java.lang.String" />
	</class>
</hibernate-mapping>
